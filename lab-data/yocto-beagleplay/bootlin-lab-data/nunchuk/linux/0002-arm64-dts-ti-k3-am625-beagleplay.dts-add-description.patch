From e1dcf8e84ab8fe139fba7b7fb2b6e4a7c6bfcd7b Mon Sep 17 00:00:00 2001
From: Jérémie Dautheribes <jeremie.dautheribes@bootlin.com>
Date: Thu, 20 Jun 2024 05:00:23 +0000
Subject: [PATCH] arm64: dts: ti/k3-am625-beagleplay.dts: add description of
 Nunchuk on i2c3

Signed-off-by: Jérémie Dautheribes <jeremie.dautheribes@bootlin.com>
---
 arch/arm64/boot/dts/ti/k3-am625-beagleplay.dts | 7 ++++++-
 1 file changed, 6 insertions(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/ti/k3-am625-beagleplay.dts b/arch/arm64/boot/dts/ti/k3-am625-beagleplay.dts
index aab0db539d01..58813b00cf00 100644
--- a/arch/arm64/boot/dts/ti/k3-am625-beagleplay.dts
+++ b/arch/arm64/boot/dts/ti/k3-am625-beagleplay.dts
@@ -979,9 +979,14 @@ it66121hdmitx_out: endpoint {
 &main_i2c3 {
 	pinctrl-names = "default";
 	pinctrl-0 = <&mikrobus_i2c_pins_default>;
-	clock-frequency = <400000>;
+	clock-frequency = <100000>;
 	status = "okay";
 	symlink = "play/mikrobus/i2c";
+
+	nunchuk: joystick@52 {
+		compatible = "nintendo,nunchuk";
+		reg = <0x52>;
+	};
 };
 
 &main_spi2 {
